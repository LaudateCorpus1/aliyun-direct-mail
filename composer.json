    "keywords": ["laravel", "directmail", "aliyun", "阿里云", "邮件推送"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tianzhao Li",
            "email": "tianzhao.li@bitmain.com"
        }
    ],
    "require": {

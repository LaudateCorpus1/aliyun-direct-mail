    },
    "require-dev": {
        "illuminate/console": "~5",
        "illuminate/support": "~5",
        "phpunit/phpunit": "~4.5",
        "illuminate/mail": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "BTCCOM\\DirectMail\\": "src/",
